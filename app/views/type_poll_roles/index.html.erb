<p id="notice"><%= notice %></p>

<h1>Listing Type Poll Roles</h1>

<table>
  <thead>
    <tr>
      <th>Type poll</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_poll_roles.each do |type_poll_role| %>
      <tr>
        <td><%= type_poll_role.type_poll %></td>
        <td><%= type_poll_role.role %></td>
        <td><%= link_to 'Show', type_poll_role %></td>
        <td><%= link_to 'Edit', edit_type_poll_role_path(type_poll_role) %></td>
        <td><%= link_to 'Destroy', type_poll_role, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

