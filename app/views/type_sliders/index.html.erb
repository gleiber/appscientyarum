<p id="notice"><%= notice %></p>

<h1>Listing Type Sliders</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_sliders.each do |type_slider| %>
      <tr>
        <td><%= type_slider.descripcion %></td>
        <td><%= link_to 'Show', type_slider %></td>
        <td><%= link_to 'Edit', edit_type_slider_path(type_slider) %></td>
        <td><%= link_to 'Destroy', type_slider, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type slider', new_type_slider_path %>
