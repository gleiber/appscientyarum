<% if notice %>
  <div class="row">
    <div class="col s12 m6">
      <div class="card blue">
        <div class="card-content white-text">
          <span class="card-title">Noticia</span>
          <p id="notice"><%= notice %></p>
        </div>
      </div>
    </div>
  </div>
<%end%>

    <div class="row z-depth-2" style="margin-top:20px;">


    <div class="col-md-10 col-md-offset-1 table-responsive col-sm-12 col-xs-12" >
     <div class="row header" style="background-color: #1f4bae;">
     <h1 class="center-align col-lg-12 text-center"></i><b>Contactos, Dudas y/o Sugerencias</b></h1>
    </div>
        <table id="tabla_vp" class="table table-hover text-center" style="margin-top: 30px">
          <thead>
            <tr>
              <th class="text-center">Email</th>
              <th class="text-center">Tipo de Contacto</th>
              <th  class="text-center" colspan="1">Acci√≥n</th>
            </tr>
          </thead>
          <tbody class="searchable">
            <% @contacts.each do |contact| %>
              <tr>
                <td><%= contact.email %></td>
                <td><%= contact.type_contact.descripcion %></td>
                <td><%= link_to contact,class: "btn btn-primary" do %>
                      <i class="fa fa-search" aria-hidden="true"></i>   
                    <% end %>
                </td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
    <style type="text/css">
  form{
    background-color:transparent !important; 
    box-shadow: none !important;
  }
</style>