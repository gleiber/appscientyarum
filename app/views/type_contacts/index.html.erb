<p id="notice"><%= notice %></p>

<h1>Type Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_contacts.each do |type_contact| %>
      <tr>
        <td><%= type_contact.descripcion %></td>
        <td><%= link_to 'Show', type_contact %></td>
        <td><%= link_to 'Edit', edit_type_contact_path(type_contact) %></td>
        <td><%= link_to 'Destroy', type_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Contact', new_type_contact_path %>
