<p id="notice"><%= notice %></p>

<h1>Cities</h1>

<%= form_tag() do %>


<% end %>
<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estados</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cities.each do |city| %>
      <tr>
        <td><%= city.descripcion %></td>
        <td><%= city.estado %></td>
        <td><%= link_to 'Show', city %></td>
        <td><%= link_to 'Edit', edit_city_path(city) %></td>
        <td><%= link_to 'Destroy', city, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New City', new_city_path %>
