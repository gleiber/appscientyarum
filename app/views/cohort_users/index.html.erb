<p id="notice"><%= notice %></p>

<h1>Cohort Users</h1>

<table>
  <thead>
    <tr>
      <th>Cohort</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cohort_users.each do |cohort_user| %>
      <tr>
        <td><%= cohort_user.cohort %></td>
        <td><%= cohort_user.user %></td>
        <td><%= link_to 'Show', cohort_user %></td>
        <td><%= link_to 'Edit', edit_cohort_user_path(cohort_user) %></td>
        <td><%= link_to 'Destroy', cohort_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cohort User', new_cohort_user_path %>
