<p id="notice"><%= notice %></p>

<h1>Listing User Area Interests</h1>

<table>
  <thead>
    <tr>
      <th>Area interest</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_area_interests.each do |user_area_interest| %>
      <tr>
        <td><%= user_area_interest.area_interest %></td>
        <td><%= user_area_interest.user %></td>
        <td><%= link_to 'Show', user_area_interest %></td>
        <td><%= link_to 'Edit', edit_user_area_interest_path(user_area_interest) %></td>
        <td><%= link_to 'Destroy', user_area_interest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User area interest', new_user_area_interest_path %>
