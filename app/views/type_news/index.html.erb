<p id="notice"><%= notice %></p>

<h1>Listing Type News</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_news.each do |type_news| %>
      <tr>
        <td><%= type_news.descripcion %></td>
        <td><%= link_to 'Show', type_news %></td>
        <td><%= link_to 'Edit', edit_type_news_path(type_news) %></td>
        <td><%= link_to 'Destroy', type_news, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type news', new_type_news_path %>
