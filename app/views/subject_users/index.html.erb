<p id="notice"><%= notice %></p>

<h1>Listing Subject Users</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subject_users.each do |subject_user| %>
      <tr>
        <td><%= subject_user.subject %></td>
        <td><%= subject_user.user %></td>
        <td><%= link_to 'Show', subject_user %></td>
        <td><%= link_to 'Edit', edit_subject_user_path(subject_user) %></td>
        <td><%= link_to 'Destroy', subject_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subject user', new_subject_user_path %>
