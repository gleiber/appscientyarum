<p id="notice"><%= notice %></p>

<h1>Listing Type Reasons</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_reasons.each do |type_reason| %>
      <tr>
        <td><%= type_reason.descripcion %></td>
        <td><%= link_to 'Show', type_reason %></td>
        <td><%= link_to 'Edit', edit_type_reason_path(type_reason) %></td>
        <td><%= link_to 'Destroy', type_reason, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type reason', new_type_reason_path %>
