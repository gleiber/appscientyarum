<p id="notice"><%= notice %></p>

<h1>Listing Type Opinions</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_opinions.each do |type_opinion| %>
      <tr>
        <td><%= type_opinion.descripcion %></td>
        <td><%= link_to 'Show', type_opinion %></td>
        <td><%= link_to 'Edit', edit_type_opinion_path(type_opinion) %></td>
        <td><%= link_to 'Destroy', type_opinion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type opinion', new_type_opinion_path %>
