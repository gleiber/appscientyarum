<p id="notice"><%= notice %></p>

<h1>Listing Question Polls</h1>

<table>
  <thead>
    <tr>
      <th>Question</th>
      <th>Poll</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @question_polls.each do |question_poll| %>
      <tr>
        <td><%= question_poll.question %></td>
        <td><%= question_poll.poll %></td>
        <td><%= link_to 'Show', question_poll %></td>
        <td><%= link_to 'Edit', edit_question_poll_path(question_poll) %></td>
        <td><%= link_to 'Destroy', question_poll, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Question poll', new_question_poll_path %>
