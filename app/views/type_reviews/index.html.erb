<p id="notice"><%= notice %></p>

<h1>Listing Type Reviews</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_reviews.each do |type_review| %>
      <tr>
        <td><%= type_review.descripcion %></td>
        <td><%= link_to 'Show', type_review %></td>
        <td><%= link_to 'Edit', edit_type_review_path(type_review) %></td>
        <td><%= link_to 'Destroy', type_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type review', new_type_review_path %>
