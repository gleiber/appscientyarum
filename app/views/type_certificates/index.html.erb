<p id="notice"><%= notice %></p>

<h1>Type Certificates</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_certificates.each do |type_certificate| %>
      <tr>
        <td><%= type_certificate.descripcion %></td>
        <td><%= link_to 'Show', type_certificate %></td>
        <td><%= link_to 'Edit', edit_type_certificate_path(type_certificate) %></td>
        <td><%= link_to 'Destroy', type_certificate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Certificate', new_type_certificate_path %>
