<p id="notice"><%= notice %></p>

<h1>Listing Classroom Laboratory Resources</h1>

<table>
  <thead>
    <tr>
      <th>Resource</th>
      <th>Classroom laboratory</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @classroom_laboratory_resources.each do |classroom_laboratory_resource| %>
      <tr>
        <td><%= classroom_laboratory_resource.resource %></td>
        <td><%= classroom_laboratory_resource.classroom_laboratory %></td>
        <td><%= classroom_laboratory_resource.state %></td>
        <td><%= link_to 'Show', classroom_laboratory_resource %></td>
        <td><%= link_to 'Edit', edit_classroom_laboratory_resource_path(classroom_laboratory_resource) %></td>
        <td><%= link_to 'Destroy', classroom_laboratory_resource, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

